{
  "name": "rearwaystartercode",
  "version": "0.0.1",
  "config": {
    "APP_NAME": "rearwaystartercode",
    "APP_VERSION": "0.0.1",
    "ENVIRONMENT": "development"
  },
  "description": "boiler plate code app backend",
  "author": "Rearway",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "script": "export $(grep -v '^#' .env | xargs) && ts-node --project tsconfig.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "APP_VERSION=$npm_package_version APP_NAME=$npm_package_name node --max-http-header-size=65536 dist/main",
    "seed:firebase": "ts-node -r dotenv/config src/scripts/seed-firebase.ts"
  },
  "dependencies": {
    "@bull-board/api": "^3.11.0",
    "@bull-board/express": "^3.11.0",
    "@bull-board/ui": "^3.11.0",
    "@google-cloud/firestore": "^7.11.0",
    "@google-cloud/logging-winston": "^4.1.1",
    "@nestjs/bull": "^0.5.5",
    "@nestjs/common": "~9.4.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "~9.4.0",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/platform-express": "~9.4.0",
    "@types/ms": "^0.7.31",
    "apollo-server-express": "^3.6.3",
    "bull": "^4.8.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.10.8",
    "firebase-admin": "^13.3.0",
    "fp-ts": "^2.11.9",
    "graphql": "^15",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "io-ts-types": "^0.5.16",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "monocle-ts": "^2.3.12",
    "ms": "^2.1.3",
    "nanoid": "^3.3.4",
    "newtype-ts": "^0.3.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.14.1"
  },
  "devDependencies": {
    "@nestjs/cli": "~9.5.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.8",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.178",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "axios": "^0.26.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "~29.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "~29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
